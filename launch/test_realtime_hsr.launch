

<launch>
 <!-- Load joint controller configurations from YAML file to parameter server -->
 <rosparam file="$(find giskardpy)/config/controller_realtime_test.yaml" command="load"/>

 <!-- load the controllers -->
 <node name="realtime_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
       output="screen" ns="/hsrb" args="realtime_arm_controller"/>

</launch>
